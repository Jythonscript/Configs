#my bindings

def_key "k"
  scroll_up

def_key "j"
  scroll_down

def_key "l"
  next_column

def_key "l"
  slave_screen

def_key "l"
  enter_directory

# scroll down 5 lines
def_key "J"
	scroll_down
	scroll_down
	scroll_down
	scroll_down
	scroll_down

# scroll up 5 lines
def_key "K"
	scroll_up
	scroll_up
	scroll_up
	scroll_up
	scroll_up

def_key "h"
  previous_column

def_key "h"
  master_screen

def_key "h"
  jump_to_parent_directory

def_key "d"
  delete_playlist_items

def_key "d"
  delete_browser_items

def_key "d"
  delete_stored_playlist

def_key "L"
	show_lyrics

def_key "F"
  jump_to_browser

def_key "g"
	move_home

def_key "G"
	move_end

def_key ";"
	select_item
